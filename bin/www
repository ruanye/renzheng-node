#!/usr/bin/env node
const app = require("../app");
function normalizePort(val) {
  var port = parseInt(val, 10);

  if (isNaN(port)) {
    // named pipe
    return val;
  }

  if (port >= 0) {
    // port number
    return port;
  }

  return false;
}

const port = normalizePort(process.env.PORT || "80");

app.listen(port, () => {
  console.log(`端口${port}正在运行`);
});
